<?xml version="1.0" encoding="utf-8" ?>
<!--Server-->
<configuration>
  <appSettings>
    <add key="aspnet:UseTaskFriendlySynchronizationContext" value="true" />
  </appSettings>
  <system.web>
    <compilation debug="true" />
  </system.web>
  <system.serviceModel>
    <bindings>
      <netTcpBinding>
        <binding name="myBinding" closeTimeout="10:10:00" openTimeout="10:10:00"
          receiveTimeout="10:10:00" sendTimeout="10:10:00" transferMode="Buffered"
          maxBufferPoolSize="2147483647" maxBufferSize="2147483647" maxReceivedMessageSize="2147483647">
          <readerQuotas maxDepth="2147483647" maxStringContentLength="2147483647"
                        maxArrayLength="2147483647" maxBytesPerRead="2147483647" maxNameTableCharCount="2147483647" />
          <reliableSession inactivityTimeout="10:10:00" />
        </binding>
      </netTcpBinding>
    </bindings>
    <behaviors>
      <serviceBehaviors>
        <behavior>
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <services>
      <service name="Filters.Service">
        <endpoint address="Filters" binding="netTcpBinding" bindingConfiguration="myBinding"
                 contract="Filters.IService"/>
        <host>
          <baseAddresses>
            <add baseAddress = "net.tcp://localhost:8733" />
          </baseAddresses>
        </host>
      </service>
    </services>
  </system.serviceModel>

</configuration>